		<script type="text/javascript">
		  var m=0;
		  var n=424;
		  var speed=20;
		function scrollPics() {
		     document.getElementById('sponsor1').style.left=m+'px';
		     document.getElementById('sponsor2').style.left=n+'px';
		   m--;
		   n--;
		if(m==-424) {
		   m=424;
		 }
		if(n==-424) {
		   n=424;
		 }
		setTimeout('scrollPics()',speed);
		 } 
		window.onload=function() {
		   scrollPics();
		 }
		</script>
